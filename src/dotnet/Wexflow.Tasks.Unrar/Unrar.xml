<?xml version="1.0" encoding="utf-8" ?>
<Tasks>
  <!--
    Unrar is a sequential task that extracts RAR archives.
   
   The files extracted are loaded by the Unrar task so that other tasks can select them through the selectFiles option.
  -->
  <Task id="$int" name="Unrar" description="$string" enabled="true|false">
    <!-- The RAR archives loaded by the task having as id $taskId will be extracted.-->
    <Setting name="selectFiles" value="$taskId" />
    <Setting name="selectFiles" value="$taskId" />
    <!-- You can add as many selecteFiles as you want.-->
    <!-- Required. Destination directory where the archives will be extracted.-->
    <Setting name="destDir" value="$string" />
  </Task>
</Tasks>