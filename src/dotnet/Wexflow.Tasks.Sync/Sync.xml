<?xml version="1.0" encoding="utf-8" ?>
<Tasks>
  <!--
    Sync a sequential task that synchronizes a source folder to a destination folder.
  -->
  <Task id="$int" name="Sync" description="$string" enabled="true|false">
    <!-- The source folder path. Example: C:\WexflowTesting\Sync_src\-->
    <Setting name="srcFolder" value="$string" />
    <!-- The destination folder path. Example: C:\WexflowTesting\Sync_dest\-->
    <Setting name="destFolder" value="$string" />
  </Task>
</Tasks>
