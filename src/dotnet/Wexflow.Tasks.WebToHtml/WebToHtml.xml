<?xml version="1.0" encoding="utf-8" ?>
<Tasks>
  <!--
    WebToHtml is a sequential task that retrieves HTML sources from urls.
    
    This task makes use of Selenium Web driver and Google Chrome driver to generate the HTML sources.
    
    The HTML files generated by this task are loaded so that other tasks can select them through selectFiles option.
  -->
  <Task id="$int" name="WebToHtml" description="$string" enabled="true|false">
    <!-- The url of the web page. Ex: https://www.google.com -->
    <Setting name="url" value="$string" />
    <Setting name="url" value="$string" />
    <!-- You can add as many urls as you want. -->
  </Task>
</Tasks>
