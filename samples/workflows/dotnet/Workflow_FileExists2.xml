	<ExecutionGraph>
		<If id="100" parent="-1" if="99">
			<Do>
				<Task id="1"><Parent id="-1" /></Task>
			</Do>
			<Else>
				<Task id="2"><Parent id="-1" /></Task>
			</Else>
		</If>
	</ExecutionGraph>
</Workflow>